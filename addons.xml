<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<addons>
<addon id="repository.mediamogul" name="Media Mogul Repo" version="1.0.1" provider-name="mediamogul">
	<extension point="xbmc.addon.repository" name="Media Mogul Repository">
		<info compressed="false">https://raw.githubusercontent.com/mmogul/mediamogul.co.uk/master/addons.xml</info>
		<checksum>https://raw.githubusercontent.com/mmogul/mediamogul.co.uk/master/addons.xml.md5</checksum>
		<datadir zip="true">https://raw.githubusercontent.com/mmogul/mediamogul.co.uk/master/zips</datadir>
	</extension>
	<extension point="xbmc.addon.metadata">
		<summary>Media Mogul</summary>
		<description>Install and Recover from Media Mogul</description>
		<platform>all</platform>
	</extension>
</addon>

<addon id="plugin.program.mediamogulwizard" name="Media Mogul Wizard" version="1.0.0" provider-name="www.mediamogul.co.uk">
  <requires>
    <import addon="xbmc.python" version="2.1.0"/>
  </requires>
  <extension point="xbmc.python.pluginsource" library="default.py">
        <provides>executable</provides>
  </extension>
  <extension point="xbmc.addon.metadata">
    <summary lang="en">Media Mogul Wizard</summary>
    <description lang="en">Easy Install With KODI / XBMC Wizard.</description>
    <platform>all</platform>
  </extension>
</addon>

<addon id="script.tvguidetecbox" name="iVue TV Guide" version="6.0.24" provider-name="TECBOX DreamHD">
	<requires>
		<import addon="xbmc.python" version="2.1.0"/>
		<import addon="script.module.simplejson" version="2.0.10"/>
	</requires>
	<extension point="xbmc.python.script" library="addon.py"/>
    <extension point="xbmc.service" library="service.py" start="login"/>
	<extension point="xbmc.addon.metadata">
		<description lang="en">iVue Guide allows you to combine some of your favourite live TV plugins for use with a fully working EPG</description>
		<disclaimer lang="en">iVue Guide is based on FTV Guide by rayw1986 and Bluezed, which in turn is based on the original TV Guide addon by twinther.</disclaimer>
		<language></language>
		<platform>all</platform>
		<license>GPLv2</license>
		<forum></forum>
		<website></website>
		<email></email>
		<source></source>
	</extension>
</addon>

</addons>
